PLAYWRIGHT_USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"

# Rate Limiting (para não ser bloqueado pelo Studus)
DELAY_MIN_ACTION=2000  # 2s mínimo entre ações
DELAY_PAGE_LOAD=5000   # 5s após carregar página
DELAY_BETWEEN_LOGIN_ATTEMPTS=300000  # 5 minutos entre tentativas de login